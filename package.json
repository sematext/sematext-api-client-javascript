{
  "name": "@sematext/sematext-api-client",
  "version": "0.3.0",
  "description": "Client library for https://sematext.com/docs/api/",
  "license": "SEE LICENSE IN LICENSE",
  "authors": {
    "name": "Sematext Packaging",
    "email": "packages.sematext.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sematext/sematext-api-client-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/sematext/sematext-api-client-javascript/issues"
  },
  "keywords": [
    "sematext",
    "apm",
    "cloud",
    "monitoring",
    "logging",
    "alerting"
  ],
  "main": "stcloud/index.js",
  "type": "module",
  "scripts": {
    "test": "mocha --require babel-core/register --recursive",
    "lint": "prettier-standard --format",
    "auto-changelog": "auto-changelog",
    "release": "release-it -VV"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "querystring": "^0.2.1",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "auto-changelog": "^2.3.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "expect.js": "^0.3.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-standard": "^13.0.6",
    "release-it": "^14.11.8",
    "sinon": "^9.2.4",
    "standard": "^16.0.4",
    "typescript": "^3.9.10"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "release-it": {
    "github": {
      "release": true,
      "tokenRef": "GITHUB_TOKEN",
      "releaseNotes": "npx auto-changelog --stdout --commit-limit false -u --template ./changelog.hbs"
    },
    "hooks": {
      "before:init": [
        "npm run lint",
        "npm test"
      ],
      "after:bump": "npx auto-changelog -p",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
    "npm": {
      "ignoreVersion": true
    }
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog.hbs",
    "commitLimit": false,
    "package": true,
    "replaceText": {
      "FIX: ": "",
      "NEW: ": "",
      "IMPROVE: ": "",
      "DOC: ": ""
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
